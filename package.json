{
  "name": "generator-swell",
  "version": "0.7.8-alpha",
  "description": "Yeoman generator for various types of Node, Angular, and Aurelia projects with TypeScript",
  "author": {
    "name": "swellaby",
    "email": "opensource@swellaby.com",
    "url": "http://swellaby.com"
  },
  "contributors": [
    {
      "name": "Caleb Cartwright",
      "url": "https://github.com/calebcartwright"
    },
    {
      "name": "Bailey Everts",
      "url": "https://github.com/beverts312"
    }
  ],
  "bugs": {
    "url": "https://github.com/swellaby/generator-swell/issues"
  },
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "generator",
    "yeoman",
    "node",
    "node.js",
    "nodejs",
    "aurelia",
    "angular",
    "angular 2",
    "typescript",
    "vsts-task",
    "vsts",
    "team services",
    "visual studio team services",
    "express",
    "cli",
    "swellaby"
  ],
  "repository": {
    "url": "https://github.com/swellaby/generator-swell",
    "type": "git"
  },
  "scripts": {
    "build": "npm run lint check-security run-unit-tests enforce-code-coverage",
    "gated-build": "gulp jshint eslint check-security run-unit-tests enforce-code-coverage",
    "check-security": "gulp check-security",
    "enforce-code-coverage": "gulp enforce-code-coverage",
    "gulp": "gulp",
    "lint": "gulp jshint eslint",
    "show-unit-test-coverage": "gulp show-unit-test-coverage-report",
    "test": "gulp run-unit-tests check-security",
    "validate": "npm ls",
    "bump-patch": "npm --no-git-tag-version version patch --force && git add package.json && git commit --no-verify --allow-empty-message -m ''"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.12.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-jshint": "^2.0.4",
    "gulp-mocha": "^3.0.1",
    "gulp-nsp": "^2.4.2",
    "gulp-open": "^2.0.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "mocha-multi": "^0.9.1",
    "precommit-hook": "^3.0.0",
    "require-dir": "^0.3.1",
    "sinon": "^1.17.6",
    "yeoman-assert": "^2.2.1",
    "yeoman-test": "^1.6.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "deep-extend": "^0.4.1",
    "glob": "^7.1.1",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "uuid": "^3.0.1",
    "yeoman-generator": "^0.24.1",
    "yosay": "^1.2.1"
  }
}
